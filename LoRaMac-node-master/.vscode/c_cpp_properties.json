{
    "configurations": [
        {
            "name": "ARM",
            "compileCommands": "${workspaceRoot}/build/compile_commands.json",
            "intelliSenseMode": "gcc-arm",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ]
            },
            "cStandard": "c11",
            "cppStandard": "c++17",
            "configurationProvider": "ms-vscode.cmake-tools",
            "defines": [
                "__CHECK_DEVICE_DEFINES",
                "__FPU_PRESENT=1U",
                "__FPU_USED=1U",
                "ARM_MATH_CM4",
                "__CMSIS_NN"
            ],
            "compilerArgs": [
                "-mthumb",
                "-mfloat-abi=hard",
                "-mfpu=fpv4-sp-d16",
                "-u _printf_float"
            ]
        }
    ],
    "version": 4
}